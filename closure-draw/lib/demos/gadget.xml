<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Closure Draw Demo"
               height="520"
               scrolling="false"
               author="WebOS Goodies"
               author_email="support@webos-goodies.jp"
               author_affiliation="WebOS Goodies"
               author_location="Tokyo, Japan"
               author_link="http://code.google.com/p/webos-goodies/">
    <Require feature="dynamic-height" />
  </ModulePrefs>
<Content type="html" view="home,profile,default,canvas"><![CDATA[
<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" href="http://closure-draw.googlecode.com/svn/trunk/closure-draw/lib/demos/from_closure_library/css/toolbar.css" />
  <link rel="stylesheet" href="http://closure-draw.googlecode.com/svn/trunk/closure-draw/lib/demos/from_closure_library/css/colormenubutton.css" />
  <link rel="stylesheet" href="http://closure-draw.googlecode.com/svn/trunk/closure-draw/lib/demos/css/closure-draw.css" />
  <style type="text/css">
	body { padding:0; margin:0; }
	#warning { color:red; text-align: center; margin: 0px 0px 1em; display:none; }
  </style>
  <script type="text/javascript" src="http://closure-draw.googlecode.com/svn/trunk/closure-draw/lib/demos/gadget-min.js"></script>
</head>
<body>
  <script type="text/javascript">
	gadgets.util.registerOnLoadHandler(function() {
      initialize();
      gadgets.window.adjustHeight();
	});
  </script>
  <div id="warning">
	Save button doesn't work on IE because of lacking SVG support :(
  </div>
  <div id="canvas"></div>
</body>
</html>
]]></Content>

</Module>
